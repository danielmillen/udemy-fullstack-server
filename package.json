{
  "name": "server",
  "version": "1.0.0",
  "description":
    "Follow-along with the Udemy fullstack developer course (https://www.udemy.com/node-with-react-fullstack-web-development/learn/v4/overview)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ficertyn/udemy-fullstack-server"
  },
  "main": "index.js",
  "engines": {
    "node": "10.1.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently  \"npm run server\" \"npm run client\"",
    "heroku-postbuild":
      "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "materialize-css": "^1.0.0-rc.1",
    "mongoose": "^5.1.2",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "stripe": "^6.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
