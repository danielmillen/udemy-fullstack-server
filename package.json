    "materialize-css": "^1.0.0-rc.1",
    "mongoose": "^5.7.5",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^4.1.1",
    "sendgrid": "^5.2.3",
    "stripe": "^6.36.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "localtunnel": "^1.9.2"
